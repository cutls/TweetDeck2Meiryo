{
  "name": "TweetDeckToMeiryo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "TweetDeckのフォントをメイリオにするだけのやつ",
  "content_scripts": [
    {
      "matches": ["https://tweetdeck.twitter.com/*"],
      "js": ["jquery.js", "index.js"],
      "run_at": "document_idle"
    }
  ]
}